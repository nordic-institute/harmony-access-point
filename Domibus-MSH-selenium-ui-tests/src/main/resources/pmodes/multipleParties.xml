        <actions>
            <action name="tc1Action" value="TC1Leg1"/>
            <action name="tc2Action" value="TC2Leg1"/>
        </actions>
        <as4>
            <receptionAwareness name="receptionAwareness" retry="0;0;CONSTANT" duplicateDetection="true"/>
            <reliability name="AS4Reliability" replyPattern="response" nonRepudiation="true"/>
            <reliability name="noReliability" replyPattern="response" nonRepudiation="false"/>
        </as4>
        <legConfigurations>
            <legConfiguration name="pushTestcase1tc1Action" reliability="noReliability" security="noSigNoEnc" receptionAwareness="receptionAwareness" service="testService1" action="tc1Action" defaultMpc="defaultMpc" propertySet="ecodexPropertySet" errorHandling="demoErrorHandling" compressPayloads="false"/>
            <legConfiguration name="pushTestcase1tc2Action" reliability="AS4Reliability" security="noSigNoEnc" receptionAwareness="receptionAwareness" service="testService1" action="tc2Action" defaultMpc="defaultMpc" propertySet="ecodexPropertySet" payloadProfile="MessageProfile" errorHandling="demoErrorHandling" compressPayloads="true"/>
