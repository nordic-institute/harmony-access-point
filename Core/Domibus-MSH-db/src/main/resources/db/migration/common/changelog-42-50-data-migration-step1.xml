    <!-- clean existing tables -->
    <changeSet author="Catalin Enache" id="EDELIVERY-7970-prepare-common">
        <comment>drop previously created tables for migration - MIGR_* and TB_D tables</comment>

        <sql endDelimiter="\n/" dbms="oracle">
DECLARE
